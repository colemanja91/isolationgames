files:
  "/etc/nginx/conf.d/nginx.custom.conf":
      mode: "644"
      owner: "root"
      group: "root"
      content: |
        proxy_connect_timeout   300;
        proxy_read_timeout      300;
        proxy_send_timeout      300;
        server {
          location / {
            proxy_http_version 1.1;
            proxy_set_header Connection "";
          }
        }
